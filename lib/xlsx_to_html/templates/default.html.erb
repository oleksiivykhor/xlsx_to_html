<table>
  <thead>
    <tr>
      <%- @headers.each do |header| -%>
        <th><%= header %></th>
      <%- end -%>
    </tr>
  </thead>
  <tbody>
    <%- @rows.each do |row| -%>
      <tr>
        <%- row.each do |cell| -%>
          <td><%= cell %></td>
        <%- end -%>
      </tr>
    <%- end -%>
  </tbody>
</table>
